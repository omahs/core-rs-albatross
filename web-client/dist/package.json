{
  "name": "@nimiq/core-web",
  "contributors": [
    "The Nimiq Core Development Team <info@nimiq.com>"
  ],
  "description": "Nimiq's Rust-to-WASM web client",
  "version": "2.0.0-alpha.13",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nimiq/core-rs-albatross"
  },
  "files": [
    "bundler",
    "types",
    "web",
    "client-proxy.js",
    "transfer-handlers.js"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./node/node/index.mjs",
        "require": "./node/node/index.js"
      },
      "browser": "./browser/bundler/index.js",
      "types": "./types/index.d.ts"
    },
    "./web": {
      "browser": "./browser/web/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "homepage": "https://nimiq.com",
  "sideEffects": [
    "./browser/bundler/main-wasm/index.js",
    "./browser/bundler/worker-wasm/index.js",
    "./node/node/main-wasm/index.js",
    "./node/node/worker-wasm/index.js",
    "./node/node/main-wasm/index.mjs",
    "./node/node/worker-wasm/index.mjs",
    "./browser/web/main-wasm/index.js",
    "./browser/web/worker-wasm/index.js"
  ],
  "keywords": [
    "nimiq",
    "cryptocurrency",
    "blockchain",
    "proof-of-stake"
  ],
  "dependencies": {
    "comlink": "^4.4.1",
    "websocket": "^1.0.34"
  }
}
